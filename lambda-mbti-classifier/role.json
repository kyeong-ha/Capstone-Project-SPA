{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Statement1",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::709182149749:root"
            },
            "Action": [
				"iam:AttachRolePolicy",
				"iam:CreateRole",
                "iam:PutRolePolicy",
				"iam:GetRole",
				"iam:DeleteRolePolicy",
                "iam:DetachRolePolicy",
				"iam:GetRolePolicy",
                "iam:ListAttachedRolePolicies",
                "iam:ListRolePolicies",
				"iam:UpdateRole",
                "iam:UpdateRoleDescription",
				"sts:AssumeRole"
			],
			"Resource": "*"
        }
    ]
}
